{
    "name": "pouler/linkfire-api",
    "description": "PHP Package for the Linkfire API",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Radt",
            "email": "paulradt@gmail.com"
        }
    ],
    "keywords": [ "linkfire", "api" ],
    "homepage": "https://github.com/PouleR/linkfire-api",
    "type": "library",
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpunit/phpunit": "^9"
    },
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "symfony/http-client": "^5.4|^6.0",
        "symfony/serializer": "^5.4|^6.0",
        "symfony/property-access": "^5.4|^6.0"
    },
    "autoload": {
        "psr-4": {
            "PouleR\\LinkfireAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PouleR\\LinkfireAPI\\Tests\\": "tests/"
        }
    }
}
